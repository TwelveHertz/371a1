module displayOperand (register);
	input [3:0] register;
	
	always @(*)
	begin
		case(register)
		4'h0: Hex = 7'b1000000;
		4'h1: Hex = 7'b1111001;
		4'h2: Hex = 7'b0100100;
		4'h3: Hex = 7'b0110000;
		4'h4: Hex = 7'b0011001;
		4'h5: Hex = 7'b0010010;
		4'h6: Hex = 7'b0000010;
		4'h7: Hex = 7'b1111000;    
		4'h8: Hex = 7'b0000000;
		4'h9: Hex = 7'b0010000;
		4'hA: Hex = 7'b0111111;
		4'hC: Hex = 7'b0000110;
		default:	Hex = 7'b1111111;
	end
		
endmodule